<androidx.appcompat.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                   android:id="@+id/toolbar2"
                                   android:layout_width="match_parent"
                                   android:layout_height="100dp"
                                   android:layout_gravity="start"
                                   android:background="@color/white"
                                   android:clickable="true"
                                   android:focusable="true"
                                   app:contentInsetEnd="0dp"
                                   app:contentInsetStart="0dp"
                                   app:popupTheme="@style/AppTheme.PopupOverlay">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:clickable="true"
            android:focusable="true"
            android:gravity="center"
            android:orientation="horizontal">

        <LinearLayout
                android:id="@+id/card_swipe_topbar_parent_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center"
                android:orientation="horizontal">

            <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="1"
            >
                <ImageView
                    android:id="@+id/card_swipe_topbar_settings"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/menu_bars"/>
                <ImageView
                        android:id="@+id/card_swipe_topbar_settings_badge"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:background="@drawable/badge"
                    android:visibility="invisible"
                    android:translationX="10dp"
                    android:layout_gravity="top" />

            </FrameLayout>

            <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="1"
            >
                <ImageView
                    android:id="@+id/card_swipe_topbar_chats"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/ladrido3_2"/>
                <ImageView
                        android:id="@+id/card_swipe_topbar_chats_badge"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:visibility="invisible"
                    android:translationX="10dp"
                    android:background="@drawable/badge"
                    android:layout_gravity="top" />

            </FrameLayout>

            <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="1"
            >
                <ImageView
                    android:id="@+id/card_swipe_topbar_logo"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/adt_old"/>
                <ImageView
                        android:id="@+id/card_swipe_topbar_logo_badge"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:translationX="10dp"
                    android:visibility="invisible"
                    android:background="@drawable/badge"
                    android:layout_gravity="top" />

            </FrameLayout>

            <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="1"
            >
                <ImageView
                    android:id="@+id/card_swipe_topbar_matches"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/heartdog2_2"/>
                <ImageView
                        android:id="@+id/card_swipe_topbar_matches_badge"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:translationX="10dp"
                    android:visibility="invisible"
                    android:background="@drawable/badge"
                    android:layout_gravity="top" />

            </FrameLayout>

            <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="48dp"
                    android:layout_weight="1"
            >
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/card_swipe_topbar_profile"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/huella"
                    app:civ_border_color="@color/black"
                    app:civ_border_width="1dp"/>
                <ImageView
                        android:id="@+id/card_swipe_topbar_profile_badge"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:translationX="10dp"
                    android:visibility="invisible"
                    android:background="@drawable/badge"
                    android:layout_gravity="top" />

            </FrameLayout>


        </LinearLayout>

    </LinearLayout>


</androidx.appcompat.widget.Toolbar>
